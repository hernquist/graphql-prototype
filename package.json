{
  "name": "mom-bucks-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon -w server/ --exec 'NODE_ENV=development node server/'",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server -s",
    "build": "yarn clean && yarn build-babel",
    "start": "yarn build && node ./build/index.js",
    "heroku:build": "yarn build && heroku login && heroku create mom-bucks-api",
    "heroku:remote": "heroku git:remote -a mom-bucks-api",
    "heroku:push": "git push heroku master"
  },
  "dependencies": {
    "64": "^0.2.0",
    "apollo-server-express": "^1.3.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
